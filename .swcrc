{
    "jsc": {
        "target": "es2017",
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": false,
            "dynamicImport": false
        },
        "keepClassNames": true,
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        },
        "baseUrl": ".",
        "paths": {
          "@/src/*": ["./*"],
          "@/util": ["util/index"],
          "@/common": ["common"]
        }
    },
    "module": {
        "type": "commonjs"
    }
}
