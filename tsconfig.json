{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./src",
    "declaration": false,
    "sourceMap": false,
    "target": "es2019",
    "module": "commonjs",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@src/*": ["src/*"]
    }
  }
}
